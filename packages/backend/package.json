{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend for the-beats-app",
  "main": "src/index.ts",
  "scripts": {
    "deploy": "cd prisma && prisma deploy",
    "generate": "cd prisma && prisma generate",
    "reset": "cd prisma && prisma reset",
    "seed": "cd prisma && prisma seed",
    "start": "docker-compose down && docker-compose up -d && ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elandamor/the-beats-app.git"
  },
  "keywords": [
    "music",
    "streaming",
    "pwa"
  ],
  "author": "Thandolwethu Mpofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elandamor/the-beats-app/issues"
  },
  "homepage": "https://github.com/elandamor/the-beats-app#readme",
  "dependencies": {
    "bcrypt": "4.0.1",
    "extendable-error": "0.1.5",
    "graphql": "15.0.0",
    "graphql-import": "1.0.2",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "merge-graphql-schemas": "1.7.8",
    "prisma-client-lib": "1.34.10"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/jsonwebtoken": "8.3.9",
    "ts-jest": "25.4.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  }
}
